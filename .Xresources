*.alpha: 0.77
!-------------
! Render
!-------------
!
Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 0
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

!https://terminal.sexy/
 
! X colors.
! Generated by 'wal'
*foreground:        #bbc7d1
*background:        #0D0C0F
*.foreground:       #bbc7d1
*.background:       #0D0C0F
emacs*foreground:   #bbc7d1
emacs*background:   #0D0C0F
URxvt*foreground:   #bbc7d1
XTerm*foreground:   #bbc7d1
UXTerm*foreground:  #bbc7d1
URxvt*background:   [100]#0D0C0F
XTerm*background:   #0D0C0F
UXTerm*background:  #0D0C0F
URxvt*cursorColor:  #bbc7d1
XTerm*cursorColor:  #bbc7d1
UXTerm*cursorColor: #bbc7d1
URxvt*borderColor:  [100]#0D0C0F

! Colors 0-15.
*.color0: #0D0C0F
*color0:  #0D0C0F
*.color1: #4C5F6B
*color1:  #4C5F6B
*.color2: #A47D5A
*color2:  #A47D5A
*.color3: #225B96
*color3:  #225B96
*.color4: #57748D
*color4:  #57748D
*.color5: #718DA3
*color5:  #718DA3
*.color6: #4896CE
*color6:  #4896CE
*.color7: #bbc7d1
*color7:  #bbc7d1
*.color8: #828b92
*color8:  #828b92
*.color9: #4C5F6B
*color9:  #4C5F6B
*.color10: #A47D5A
*color10:  #A47D5A
*.color11: #225B96
*color11:  #225B96
*.color12: #57748D
*color12:  #57748D
*.color13: #718DA3
*color13:  #718DA3
*.color14: #4896CE
*color14:  #4896CE
*.color15: #bbc7d1
*color15:  #bbc7d1

! Black color that will not be affected by bold highlighting.
*.color66: #0D0C0F
*color66:  #0D0C0F

! Xclock colors.
XClock*foreground: #bbc7d1
XClock*background: #0D0C0F
XClock*majorColor:  rgba:bb/c7/d1/ff
XClock*minorColor:  rgba:bb/c7/d1/ff
XClock*hourColor:   rgba:bb/c7/d1/ff
XClock*minuteColor: rgba:bb/c7/d1/ff
XClock*secondColor: rgba:bb/c7/d1/ff

! Set depth to make transparency work.
URxvt*depth: 32
